{
	"index": "index.html",
	"method": "GET POST",
	"python_cgi_extension": "py",
	"perl_cgi_extension": "pl",
	"upload_root": "./www/cgi/upload",
	"root": "./www/",
	"uploadable": false,
	"autoindex": false,
	"client_body_size": "100000000",
	"route index": {
		"path": "/",
		"autoindex": true
	},
	"route post": {
		"uploadable": true,
		"path": "/post",
		"index": "post.html"
	},
	"route autoindex": {
		"path": "/autoindex",
		"method": "GET POST DELETE",
		"autoindex": true,
		"index": "",
		"root": "./www/autoindex"
	},
	"route bonus": {
		"path": "/bonus",
		"index": "bonus.html"
	},
	"route min-kang": {
		"path": "/min-kang",
		"redirect": "http://www.github.com/mgkgng"
	},
	"route sspina": {
		"path": "/sspina",
		"redirect": "http://www.github.com/salvatorespina"
	},
	"route jrathelo": {
		"path": "/jrathelo",
		"redirect": "http://www.github.com/blenderwizard"
	},
	"code 404": {
		"code": 404,
		"redirect": "/404"
	},
	"route 404": {
		"index": "error_404.html",
		"path": "/404",
		"root": "./www/error_pages"
	},
	"server0": {
		"host": "127.0.0.1",
		"port": 42069
	},
	"server1": {
		"host": "127.0.0.1",
		"port": 42071
	}
}